{
  "name": "TGP",
  "type": "project",
  "description": "The Generations Project",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Toby Fox",
      "email": "tobyfox@gmail.com"
    },
    {
      "name": "Robin Dogrucu",
      "email": "robindogrucu@gmail.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "braintree/braintree_php" : "3.23.0",
    "composer/installers": "~1.2.0",
    "vlucas/phpdotenv": "^2.4.0",
    "johnpbloch/wordpress": "4.7.2",
    "oscarotero/env": "^1.0",
    "wpackagist-plugin/akismet": "~3.3",
    "wpackagist-plugin/better-wp-security": "6.2.0",
    "wpackagist-plugin/contact-form-7": "~4.7",
    "wpackagist-plugin/featured-image-column": "~0.2.3",
    "wpackagist-plugin/gallery-slideshow": "~1.3.1",
    "wpackagist-plugin/hello-dolly": "*",
    "wpackagist-plugin/regenerate-thumbnails": "~2.2.6",
    "wpackagist-plugin/rich-text-excerpts": "~1.3.3",
    "wpackagist-plugin/wordpress-seo": "~4.6",
    "wpackagist-plugin/wp-ses": "~0.4.9"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
